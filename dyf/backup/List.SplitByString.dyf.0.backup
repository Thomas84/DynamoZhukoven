      <PortInfo index="1" default="False" />
      <Script>from itertools import groupby

oldList = IN[0]
def breakby(oldList, sep, delim=None):
	for i in oldList:
		p = i.split(sep)
		yield p[0]
		if len(p) &gt; 1:
			yield delim
			yield p[1]
newList = [list(g) for k,g in groupby(breakby(oldList, IN[1], None), lambda x: x is not None) if k]

OUT = newList</Script>
